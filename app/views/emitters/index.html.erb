<div class="seccion">
  <table>
  <tr>
    <th>Id</th>
    <th>Nombre</th>
    <th>Descripci&oacute;n</th>
    <th>Estado</th>
    <th></th>
  </tr>

  <% @emitters.each do |emitter| %>
  <tr>
    <td><%= emitter.id %></td>
    <td><%= emitter.name %></td>
    <td><%= emitter.description %></td>
    <td><% if emitter.state %>Activo<% else %>Inactivo<% end %></td>
    <td><%= link_to 'Ver', emitter %></td>
  </tr>
  <% end %>
  </table>
</div>

<div class="botones">
  <%= link_to 'Crear Emisor', new_emitter_path %>
  <%= render 'shared/pp_adm_dsg' %>
</div>

